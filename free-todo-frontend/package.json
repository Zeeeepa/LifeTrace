{
	"name": "free-todo-frontend",
	"version": "0.1.0",
	"private": true,
	"main": "dist-electron/main.js",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "biome lint ./app ./apps ./components ./lib",
		"format": "biome format --write ./app ./apps ./components ./lib",
		"check": "biome check ./app ./apps ./components ./lib",
		"type-check": "tsc --noEmit",
		"api:generate": "orval",
		"api:generate:watch": "orval --watch",
		"electron:build-main": "node scripts/build-electron.js",
		"electron:resolve-symlinks": "node scripts/resolve-symlinks.js",
		"electron:copy-missing-deps": "node scripts/copy-missing-deps.js",
		"electron:dev": "pnpm electron:build-main && electron .",
		"electron:build": "pnpm build && pnpm electron:resolve-symlinks && pnpm electron:copy-missing-deps && pnpm electron:build-main && electron-builder",
		"electron:build-win": "pnpm build && pnpm electron:resolve-symlinks && pnpm electron:copy-missing-deps && pnpm electron:build-main && electron-builder --win",
		"electron:build-mac": "pnpm build && pnpm electron:resolve-symlinks && pnpm electron:copy-missing-deps && pnpm electron:build-main && electron-builder --mac",
		"electron:build-linux": "pnpm build && pnpm electron:resolve-symlinks && pnpm electron:copy-missing-deps && pnpm electron:build-main && electron-builder --linux"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@tanstack/react-query": "^5.90.12",
		"clsx": "^2.1.1",
		"dayjs": "^1.11.19",
		"framer-motion": "^12.23.26",
		"lucide-react": "^0.561.0",
		"next": "16.0.10",
		"next-themes": "^0.4.6",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-markdown": "^10.1.0",
		"remark-gfm": "^4.0.1",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.2.1",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/typography": "^0.5.19",
		"@types/node": "^25.0.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"autoprefixer": "^10.4.22",
		"baseline-browser-mapping": "^2.9.7",
		"electron": "^39.2.7",
		"electron-builder": "^26.0.12",
		"esbuild": "^0.27.1",
		"orval": "^7.17.0",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3"
	}
}
